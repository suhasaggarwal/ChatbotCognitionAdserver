<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>Controller</servlet-name>
    <servlet-class>com.cuberoot.SBanner.Controller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/Controller</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdTracking</servlet-name>
    <servlet-class>com.cuberoot.SBanner.AdTracking</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdTracking</servlet-name>
    <url-pattern>/AdTracking</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>ResponseTracking</servlet-name>
    <servlet-class>com.cuberoot.SBanner.ResponseTracking</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResponseTracking</servlet-name>
    <url-pattern>/ResponseTracking</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ContextRefresh</servlet-name>
    <servlet-class>com.cuberoot.SBanner.ContextRefresh</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContextRefresh</servlet-name>
    <url-pattern>/ContextRefresh</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GeoInfo</display-name>
    <servlet-name>GeoInfo</servlet-name>
    <servlet-class>com.cuberoot.SBanner.GeoInfo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GeoInfo</servlet-name>
    <url-pattern>/GeoInfo</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>